{
    "name": "r4gmi-auth-ui",
    "version": "1.0.0",
    "scripts": {
        "css": "postcss styles.css -o ./dist/css/styles.css",
        "dev": "rimraf dist && npm-run-all --parallel css rollup:dev",
        "build": "rimraf dist && npm-run-all --parallel css rollup:build",
        "rollup:dev": "rollup -c rollup.dev.js",
